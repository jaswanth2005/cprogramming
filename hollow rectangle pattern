#include<stdio.h>
int main()
{
	int rows,columns,i,j;
	printf("Enter the number of rows  :");
	scanf("%d",&rows);
	printf("Enter the number of columns  :");
	scanf("%d",&columns);
	
	for(i=0; i<rows; i++)
	{
		for(j=0;j<columns; j++)
		{
			
			if(i==0 || i==rows-1 || j==0 || j==columns-1)
				printf("*");
			else
				printf(" ");
		}
		printf("\n");
	}
	return 0;	
}
