#include<stdio.h>
#include<conio.h>
int main()
{
int size,i,a[10]; 
printf("\nplease enter the size of an array:");
scanf("%d",&size);
printf("\n please enter the array elements:");
for(i=0;i<size;i++)
{
scanf("%d",&a[i]);
}
printf("\n list of negative numbers in this array;");
for(i=0;i<size;i++)
{
if(a[i]<0)
{
	printf("%d",a[i]);
	}
}
return 0;
}
